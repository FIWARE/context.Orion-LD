# VALGRIND_READY - to mark the test ready for valgrindTestSuite.sh

--NAME--
Retrieval of Context Source Registrations

--SHELL-INIT--
dbInit CB
brokerStart CB

--SHELL--

#
# 01. GET  /ngsi-ld/v1/csourceRegistrations GET empty data
# 02. POST /ngsi-ld/v1/csourceRegistrations CSR:1,2,3,5... 10
# 03. GET  /ngsi-ld/v1/csourceRegistrations GET all data
# 04. GET  /ngsi-ld/v1/csourceRegistrations GET limit:5 csourceRegistration
# 05. GET  /ngsi-ld/v1/csourceRegistrations GET limit:5/offset:0 csourceRegistration
# 06. GET  /ngsi-ld/v1/csourceRegistrations GET limit:5/offset:1 csourceRegistration
# 07. GET  /ngsi-ld/v1/csourceRegistrations GET options=count csourceRegistration
#

echo "01. GET  /ngsi-ld/v1/csourceRegistrations empty data"
echo "===================================================="
orionCurl --url /ngsi-ld/v1/csourceRegistrations
echo
echo

echo "02. POST /ngsi-ld/v1/csourceRegistrations CSR1"
echo "=============================================="
typeset -i csrNo;
csrNo=1
while [ $csrNo -le 10 ]
	do
		payload='{
            "id": "urn:ngsi-ld:ContextSourceRegistration:CSR'$csrNo'",
            "type": "ContextSourceRegistration",
            "description": "Test", 
            "information": [{
                    "entities": [{
                        "id": "urn:ngsi-ld:Vehicle:V'$csrNo'",
                        "type": "Vehicle"
                    }]
                },
                {
                    "entities": [{
                            "idPattern": ".*downtown$",
                            "type": "OffStreetParking"
                        },
                        {
                            "idPattern": ".*47$",
                            "type": "OffStreetParking"
                        }
                    ]
                }
            ],
            "observationInterval": {
                "start": "2018-12-31T10:00:00",
                "end": "2018-12-31T10:00:00"
            },
            "managementInterval": {
                "start": "2018-12-31T10:00:00",
                "end": "2018-12-31T10:00:00"
            },
            "endpoint": "http://my.csource.org:1026",
            "@context": [
                "http://uri.etsi.org/ngsi-ld/v1/ngsi-ld-core-context.jsonld"
            ]
        }'
		orionCurl --url /ngsi-ld/v1/csourceRegistrations -X POST --payload "$payload" -H "Content-Type: application/ld+json" -H "Accept: application/ld+json" --linkHeaderFix
		csrNo=$csrNo+1
	done
echo
echo

echo "03. GET  /ngsi-ld/v1/csourceRegistrations all data"
echo "=================================================="
orionCurl --url /ngsi-ld/v1/csourceRegistrations
echo
echo

echo "04. GET  /ngsi-ld/v1/csourceRegistrations limit:5 csourceRegistration"
echo "====================================================================="
orionCurl --url '/ngsi-ld/v1/csourceRegistrations?limit=5' -H 'Link: <http://uri.etsi.org/ngsi-ld/v1/ngsi-ld-core-context.jsonld>; rel="http://www.w3.org/ns/json-ld#context"; type="application/ld+json"'
echo
echo

echo "05. GET  /ngsi-ld/v1/csourceRegistrations limit:5/offset:0 csourceRegistration"
echo "=============================================================================="
orionCurl --url '/ngsi-ld/v1/csourceRegistrations?limit=5&offset=0' -H 'Link: <http://uri.etsi.org/ngsi-ld/v1/ngsi-ld-core-context.jsonld>; rel="http://www.w3.org/ns/json-ld#context"; type="application/ld+json"'
echo
echo

echo "06. GET  /ngsi-ld/v1/csourceRegistrations limit:5/offset:1 csourceRegistration"
echo "=============================================================================="
orionCurl --url '/ngsi-ld/v1/csourceRegistrations?limit=5&offset=1' -H 'Link: <http://uri.etsi.org/ngsi-ld/v1/ngsi-ld-core-context.jsonld>; rel="http://www.w3.org/ns/json-ld#context"; type="application/ld+json"'
echo
echo

echo "07. GET  /ngsi-ld/v1/csourceRegistrations GET options=count csourceRegistration"
echo "==============================================================================="
orionCurl --url '/ngsi-ld/v1/csourceRegistrations?limit=3&options=count' -H 'Link: <http://uri.etsi.org/ngsi-ld/v1/ngsi-ld-core-context.jsonld>; rel="http://www.w3.org/ns/json-ld#context"; type="application/ld+json"'
echo
echo


